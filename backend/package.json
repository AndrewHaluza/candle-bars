{
  "name": "express-ts-server",
  "version": "1.0.0",
  "description": "A simple Express server built with TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/app.js",
    "dev": "ts-node-dev --respawn --env-file=.env --transpile-only  src/app.ts ",
    "watch": "tsc --watch",
    "simulator": "node --env-file=.env scripts/weather_stream_simulator.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^16.11.7",
    "@types/sqlite3": "^3.1.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/express-ts-server.git"
  },
  "keywords": [
    "express",
    "typescript",
    "server"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/express-ts-server/issues"
  },
  "homepage": "https://github.com/yourusername/express-ts-server#readme"
}
